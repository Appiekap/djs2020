@model IEnumerable<DemoWebApp.Models.Book>
@{
    ViewData["Title"] = "Book Overview";
}

<h1>Overview of Books</h1>

<table>
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(model => model.Id)</th>
            <th>@Html.DisplayNameFor(model => model.Title)</th>
            <th>@Html.DisplayNameFor(model => model.Author)</th>
            <th>@Html.DisplayNameFor(model => model.PublishedDate)</th>
        </tr>        
    </thead>

    @foreach (var book in Model)
    {
<tr>
    <td>@book.Id</td>
    <td>@book.Title</td>
    <td>@book.Author</td>
    <td>@Html.DisplayFor(modelItem => book.PublishedDate)</td>
</tr>
    }

</table>